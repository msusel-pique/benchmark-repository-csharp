<!DOCTYPE html>
<!--[if IE 9 ]><html class="ie ie9" lang="en" class="no-js"> <![endif]-->
<!--[if !(IE)]><!-->
<html lang="en" class="no-js">
 <!--<![endif]-->
<head>
    <title>@ViewBag.Title</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="author" content="Production Ready, LLC - http://www.ProductionReady.com">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="/images/ProductionReady/EleflexIcon144x144.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="/images/ProductionReady/EleflexIcon114x114.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="/images/ProductionReady/EleflexIcon72x72.png">
    <link rel="apple-touch-icon-precomposed" sizes="57x57" href="/images/ProductionReady/EleflexIcon57x57.png">
    <link rel="shortcut icon" href="/images/ProductionReady/favicon.ico">    
    @Styles.Render("~/Content/DataTables/cssEleflex")
    @Styles.Render("~/Content/bootstrapchosenEleflex")
    @Styles.Render("~/Content/fontawesomeEleflex")
    @Styles.Render("~/Content/smalot-datetimepicker/datetimepickerEleflex")
    @Styles.Render("~/Content/bootstrapEleflex")    
    @Styles.Render("~/Content/cssEleflex")
    @Styles.Render("~/Content/themes/base/cssEleflex")
    @Scripts.Render("~/bundles/modernizrEleflex")    
    @Scripts.Render("~/bundles/jqueryEleflex")
    @Scripts.Render("~/bundles/jqueryuiEleflex")
    @Scripts.Render("~/bundles/jqueryvalEleflex")
    @Scripts.Render("~/bundles/datatablejsEleflex")
    @Scripts.Render("~/bundles/chosenjsEleflex")
    @Scripts.Render("~/bundles/bootstrapEleflex")
    @Scripts.Render("~/bundles/knockoutjsEleflex")
    @Scripts.Render("~/bundles/momentjsEleflex")
    @Scripts.Render("~/bundles/datetimepickerjsEleflex")
    @Scripts.Render("~/bundles/jsEleflex")
    @RenderSection("styles", required: false)
</head>
<body id="body">
    <a class="sr-only sr-only-focusable" href="#MasterBody">Skip to main content</a>
    <div id="MasterRoot" class="LayoutMasterRoot container">
        <header id="MasterHeader" class="LayoutMasterHeader">
            @Html.Partial("_EleflexHeaderPartial")
        </header>
        <div id="MasterBody" class="LayoutMasterBody">
            <div id="MasterAlertMessage"></div>            
            @RenderBody()
        </div>
        <footer id="MasterFooter" class="LayoutMasterFooter">
            @Html.Partial("_EleflexFooterPartial")
        </footer>
    </div>
@RenderSection("scripts", required: false)
</body>
</html>
